<ROOT id="toolSettingsFull" defaultPosition="100,300">
<V_BOX id="tsf_wrapper">
	
	<H_BOX id="tsf_tabWrapper" class="tabWrapper" ondragstart="movePanel(0)" ondrag="movePanel(1)">
		<H_BOX class="panelTab">
			<TEXT class="panelTabText" value="TOOL SETTINGS" ></TEXT>
		</H_BOX>
		<H_BOX class="tabButton"></H_BOX>
	</H_BOX>
	<H_BOX id="tsf_body">
		
		////////////////////////////////////////////
		// INTEREST TABS BLOCK
		////////////////////////////////////////////
	
		<V_BOX id="tsf_interestWrapper">
			// Meta Data
			
			<H_BOX id="tsf_interestMetadata" class="tsf_interest" style="visibility:visible;">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(0, &quot;tsf_interestMetadata&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="METADATA"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
		// Controls
			// Continuous 1
			<H_BOX id="tsf_interestControl_continuous" class="tsf_interest" style="margin-top:0;">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(1, &quot;tsf_interestControl_continuous&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="CONTINUOUS"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Drag 2
			<H_BOX id="tsf_interestControl_drag" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(2, &quot;tsf_interestControl_drag&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="DRAG"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// One Point 3
			<H_BOX id="tsf_interestControl_onePoint" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(3, &quot;tsf_interestControl_onePoint&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="ONE POINT"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Two Point 4
			<H_BOX id="tsf_interestControl_twoPoint" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(4, &quot;tsf_interestControl_twoPoint&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="TWO POINT"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Three Point 5
			<H_BOX id="tsf_interestControl_threePoint" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(5, &quot;tsf_interestControl_threePoint&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="THREE POINT"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Zoom 6
			<H_BOX id="tsf_interestControl_zoom" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(6, &quot;tsf_interestControl_zoom&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="ZOOM"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Pan 7
			<H_BOX id="tsf_interestControl_pan" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(7, &quot;tsf_interestControl_pan&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="PAN"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Rotate 8
			<H_BOX id="tsf_interestControl_rotate" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(8, &quot;tsf_interestControl_rotate&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="ROTATE"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Sampler 9
			<H_BOX id="tsf_interestControl_sampler" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(9, &quot;tsf_interestControl_sampler&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="SAMPLER"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
		// Input Masks (Cannot be disabled)
			// Rake 30
			<H_BOX id="tsf_interestRake" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(30, &quot;tsf_interestRake&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="RAKE"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
		// Other Masks (Some can be disabled)
			// Basic 11
			<H_BOX id="tsf_interestBasic" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(11, &quot;tsf_interestBasic&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="BASIC"></TEXT></H_BOX>
				<H_BOX class="tsf_interestBox"><IMAGE class="tsf_interestCheck_static"></IMAGE></H_BOX>
			</H_BOX>
			// Image 12
			<H_BOX id="tsf_interestImage" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(12, &quot;tsf_interestImage&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="IMAGE"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;image&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;image&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;image&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			// Character 13
			<H_BOX id="tsf_interestCharacter" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(13, &quot;tsf_interestCharacter&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="CHARACTER"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;character&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;character&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;character&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			// Alpha 15
			<H_BOX id="tsf_interestAlpha" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(15, &quot;tsf_interestAlpha&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="ALPHA"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;alpha&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;alpha&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;alpha&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			// Color 16
			<H_BOX id="tsf_interestColor" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(16, &quot;tsf_interestColor&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="COLOR"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;color&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;color&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;color&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			// Scatter 17
			<H_BOX id="tsf_interestScatter" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(17, &quot;tsf_interestScatter&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="SCATTER"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;scatter&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;scatter&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;scatter&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			// Effects 24
			<H_BOX id="tsf_interestEffects" class="tsf_interest">
				<H_BOX class="tsf_interestName" 
					onmouseenter="tsf_hoverInterest(1)" 
					onmouseleave="tsf_hoverInterest(0)" 
					onclick="tsf_clickInterest(24, &quot;tsf_interestEffects&quot;)">
					<TEXT class="barlow12, tsf_interestText" value="EFFECTS"></TEXT></H_BOX>
				<H_BOX onclick="tsf_toggleInterest(&quot;effects&quot;)" class="tsf_interestBox">
					<IMAGE class="tsf_interestCheck"
						onmouseenter="tsf_hoverInterestCheck(0, &quot;effects&quot;)"
						onmouseleave="tsf_hoverInterestCheck(1, &quot;effects&quot;)"></IMAGE>
				</H_BOX>
			</H_BOX>
			
		</V_BOX>
		
		////////////////////////////////////////////
		// SETTINGS BLOCK
		////////////////////////////////////////////
		
		<V_BOX id="tsf_settingsContainer" class="tsf_settingsWrapper">
			
			
			
			////////////////////////////////////////////
			// META DATA INTEREST MASK
			////////////////////////////////////////////
			<V_BOX id="tsf_bodyMetadata" class="set_dynBlockWrapper">
			
				// META DATA
				<V_BOX class="tsf_settingsBlock">
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="ID:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:42;">
							<LINE id="tsf_meta_ID" class="tsf_inputText, barlow12, textInputColor" 
								onentry="setToolData_Meta(0, -1, true)" oncancel="setToolData_Meta(0, -1, false)" value="-1"></LINE>
						</H_BOX>
						<TEXT class="tsf_settingsText, barlow12" value="NAME:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:fill;">
							<LINE id="tsf_meta_name" class="tsf_inputText, barlow12, textInputColor" 
								onentry="setToolData_Meta(3, -1, true)" oncancel="setToolData_Meta(3, -1, false)" value="Tool name..."></LINE>
						</H_BOX>
					</H_BOX>
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="TAGS:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:fill;">
							<LINE id="tsf_meta_tags" class="tsf_inputText, barlow12, textInputColor" value="Comma seperated tags..."></LINE>
						</H_BOX>
					</H_BOX>
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="CATEGORY:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:fill;">
							<LINE id="tsf_meta_category" class="tsf_inputText, barlow12, textInputColor" 
								onentry="setToolData_Meta(16, -1, true)" oncancel="setToolData_Meta(16, -1, false)" value="Category..."></LINE>
						</H_BOX>
					</H_BOX>
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="DATE:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:82;">
							<LINE id="tsf_meta_date" class="tsf_inputText, barlow12, textInputColor" 
								onentry="setToolData_Meta(18, -1, true)" oncancel="setToolData_Meta(18, -1, false)" value="Date created..."></LINE>
						</H_BOX>
						<TEXT class="tsf_settingsText, barlow12" value="AUTHOR:"></TEXT>
						<H_BOX class="tsf_inputBox" style="width:fill;">
							<LINE id="tsf_meta_author" class="tsf_inputText, barlow12, textInputColor" 
								onentry="setToolData_Meta(19, -1, true)" oncancel="setToolData_Meta(19, -1, false)" value="Tool author..."></LINE>
						</H_BOX>
					</H_BOX>
				</V_BOX>
				<H_BOX class="tsf_settingsDivider"></H_BOX>
				
				// COLOR AND ICON
				<V_BOX class="tsf_settingsBlock">
					<H_BOX class="tsf_settingsLine">
						<H_BOX style="height:inherit;min-width:24;overflow-X:auto;float:center;">
							<TEXT class="tsf_settingsText, barlow12" value="ICON:"></TEXT>
							<IMAGE class="tsf_checkBox" value="off"></IMAGE>
							<TEXT class="tsf_settingsText, barlow12" value="USE TIP:"></TEXT>
							<IMAGE class="tsf_checkBox" value="off"></IMAGE>
							<TEXT class="tsf_settingsText, barlow12" value="ICON HIGLIGHT:"></TEXT>
							<IMAGE class="tsf_checkBox" value="off"></IMAGE>
							<TEXT class="tsf_settingsText, barlow12" value="ICON FILL:"></TEXT>
							<IMAGE class="tsf_checkBox" value="off"></IMAGE>
						</H_BOX>
					</H_BOX>
				</V_BOX>
				<H_BOX class="tsf_settingsDivider"></H_BOX>
				
				// KEYBINDER
				<V_BOX class="tsf_settingsBlock">
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="BINDER:"></TEXT>
						<H_BOX id="tsf_meta_binderWrapper" class="tsf_dropWrapper" style="width:96;"
						onmouseenter="global::hoverDropdown(0)" 
						onmouseleave="global::hoverDropdown(1)"
						onfocus="toggleSimpleDrop(0, &quot;tsf_dropList&quot;)"
						onblur="toggleSimpleDrop(1, &quot;tsf_dropList&quot;)">
							<V_BOX class="tsf_dropTop" style="width:78;">
								<TEXT id="tsf_meta_bindType" class="tsf_dropText, barlow12" value="STANDARD"></TEXT>
								<V_BOX class="tsf_dropList" onmouseleave="hoverDropOptions(5, 1, &quot;STANDARD&quot;, &quot;tsf_meta_binderWrapper&quot;)">
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(5, 0, &quot;STANDARD&quot;, &quot;tsf_meta_binderWrapper&quot;)"
									onclick="clickDropOption(5, &quot;STANDARD&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="STANDARD"></TEXT></H_BOX>
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(5, 0, &quot;ALPHABET&quot;, &quot;tsf_meta_binderWrapper&quot;)"
									onclick="clickDropOption(5, &quot;ALPHABET&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="ALPHABET"></TEXT></H_BOX>
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(5, 0, &quot;SWITCH&quot;, &quot;tsf_meta_binderWrapper&quot;)"
									onclick="clickDropOption(5, &quot;SWITCH&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="SWITCH"></TEXT></H_BOX>
								</V_BOX>
							</V_BOX>
							<IMAGE class="tsf_dropButton"></IMAGE>
						</H_BOX>
						<H_BOX id="tsf_meta_bindStandard" class="tsf_bindOption">
							<TEXT class="tsf_settingsText, barlow12" value="KEY:"></TEXT>
							<H_BOX id="tsf_stdKeyBox" class="tsf_inputBox" style="width:fill;"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="beginKeyCapture(&quot;tsf_meta_keyStandard&quot;, &quot;tsf_stdKeyBox&quot;, &quot;enter key...&quot;, 1, tsf_meta_getKey)">
								<TEXT id="tsf_meta_keyStandard" class="tsf_dropText, barlow12" value="SET KEY..."></TEXT>
							</H_BOX>
						</H_BOX>
						<H_BOX id="tsf_meta_bindAlphabet" class="tsf_bindOption" style="visibility:hide;">
							<TEXT class="tsf_settingsText, barlow12" value="1:" style="border-right:2;"></TEXT>
							<H_BOX id="tsf_alphaKeyBox_1" class="tsf_inputBoxSmall"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="meta_alphaIndex = 0; beginKeyCapture(&quot;tsf_meta_keyAlphabet_1&quot;, &quot;tsf_alphaKeyBox_1&quot;, &quot;~&quot;, 2, tsf_meta_getAlphaKey);">
								<TEXT id="tsf_meta_keyAlphabet_1" class="tsf_dropText, barlow12" value="-"></TEXT>
							</H_BOX>
							<TEXT class="tsf_settingsText, barlow12" value="2:" style="border-right:2;"></TEXT>
							<H_BOX id="tsf_alphaKeyBox_2" class="tsf_inputBoxSmall"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="meta_alphaIndex = 1; beginKeyCapture(&quot;tsf_meta_keyAlphabet_2&quot;, &quot;tsf_alphaKeyBox_2&quot;, &quot;~&quot;, 2, tsf_meta_getAlphaKey);">
								<TEXT id="tsf_meta_keyAlphabet_2" class="tsf_dropText, barlow12" value="-"></TEXT>
							</H_BOX>
							<TEXT class="tsf_settingsText, barlow12" value="3:" style="border-right:2;"></TEXT>
							<H_BOX id="tsf_alphaKeyBox_3" class="tsf_inputBoxSmall"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="meta_alphaIndex = 2; beginKeyCapture(&quot;tsf_meta_keyAlphabet_3&quot;, &quot;tsf_alphaKeyBox_3&quot;, &quot;~&quot;, 2, tsf_meta_getAlphaKey);">
								<TEXT id="tsf_meta_keyAlphabet_3" class="tsf_dropText, barlow12" value="-"></TEXT>
							</H_BOX>
							<TEXT class="tsf_settingsText, barlow12" value="4:" style="border-right:2;"></TEXT>
							<H_BOX id="tsf_alphaKeyBox_4" class="tsf_inputBoxSmall"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="meta_alphaIndex = 3; beginKeyCapture(&quot;tsf_meta_keyAlphabet_4&quot;, &quot;tsf_alphaKeyBox_4&quot;, &quot;~&quot;, 2, tsf_meta_getAlphaKey);">
								<TEXT id="tsf_meta_keyAlphabet_4" class="tsf_dropText, barlow12" value="-"></TEXT>
							</H_BOX>
						</H_BOX>
						<H_BOX id="tsf_meta_bindSwitch" class="tsf_bindOption" style="visibility:hide;">
							<TEXT class="tsf_settingsText, barlow12" value="KEY:"></TEXT>
							<H_BOX id="tsf_swKeyBox" class="tsf_inputBox" style="width:fill;"
							onmouseenter="global::hoverMidGreyButton(0)" 
							onmouseleave="global::hoverMidGreyButton(1)"
							onclick="beginKeyCapture(&quot;tsf_meta_keySwitch&quot;, &quot;tsf_swKeyBox&quot;, &quot;enter key...&quot;, 3, tsf_meta_getKey);">
								<TEXT id="tsf_meta_keySwitch" class="tsf_dropText, barlow12" value="SET KEY..."></TEXT>
							</H_BOX>
						</H_BOX>
					</H_BOX>
				</V_BOX>
				<H_BOX class="tsf_settingsDivider"></H_BOX>
				
				// CURSOR SETTINGS
				<V_BOX class="tsf_settingsBlock">
					<H_BOX class="tsf_settingsLine">
						<TEXT class="tsf_settingsText, barlow12" value="HOVER:"></TEXT>
						<H_BOX id="tsf_meta_cursorHoverWrapper" class="tsf_dropWrapper" style="width:96;"
						onmouseenter="global::hoverDropdown(0)" 
						onmouseleave="global::hoverDropdown(1)"
						onfocus="toggleSimpleDrop(0, &quot;tsf_dropList&quot;)"
						onblur="toggleSimpleDrop(1, &quot;tsf_dropList&quot;)">
							<V_BOX class="tsf_dropTop" style="width:78;">
								<TEXT id="tsf_meta_cursorHover" class="tsf_dropText, barlow12, textInputColor" value="POINTER"></TEXT>
								<V_BOX class="tsf_dropList" onmouseleave="hoverDropOptions(3, 1, &quot;POINTER&quot;, &quot;tsf_meta_cursorHoverWrapper&quot;)">
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(3, 0, &quot;POINTER&quot;, &quot;tsf_meta_cursorHoverWrapper&quot;)"
									onclick="clickDropOption(3, &quot;POINTER&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="POINTER"></TEXT></H_BOX>
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(3, 0, &quot;HAND&quot;, &quot;tsf_meta_cursorHoverWrapper&quot;)"
									onclick="clickDropOption(3, &quot;HAND&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="HAND"></TEXT></H_BOX>
									<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(3, 0, &quot;GRAB&quot;, &quot;tsf_meta_cursorHoverWrapper&quot;)"
									onclick="clickDropOption(3, &quot;GRAB&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="GRAB"></TEXT></H_BOX>
										<H_BOX class="tsf_dropItem"
									onmouseenter="hoverDropOptions(3, 0, &quot;ROTATE&quot;, &quot;tsf_meta_cursorHoverWrapper&quot;)"
									onclick="clickDropOption(3, &quot;ROTATE&quot;, true)">
										<TEXT class="barlow12, tsf_dropText, tsf_sub" value="ROTATE"></TEXT></H_BOX>
										<H_BOX class="tsf_dropItem"
