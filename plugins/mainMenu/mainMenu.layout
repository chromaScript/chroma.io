	<H_BOX id="mm_menuWinButtonWrapper" onmouseleave="winButtonHover(1)">
		<H_BOX class="mm_button" onmouseenter="winButtonHover(0)" onclick="app.minimize()">
			<IMAGE class="mm_buttonImage" id="mm_minimize"></IMAGE>
		</H_BOX>
		<H_BOX class="mm_button" onmouseenter="winButtonHover(0)" onclick="app.maximize()">
			<IMAGE class="mm_buttonImage" id="mm_maximize"></IMAGE>
		</H_BOX>
		<H_BOX class="mm_button" onmouseenter="winButtonHover(0)" onrelease="app.exit()">
			<IMAGE class="mm_buttonImage" id="mm_close"></IMAGE>
		</H_BOX>
	</H_BOX>
</H_BOX>
</ROOT>

