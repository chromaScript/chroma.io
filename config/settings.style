.set_blockWrapper
{
	width: inherit;
	min-height: 14;
	margin-left: 4;
	margin-top: 3;
	margin-bottom: 4;
	overflow-Y: auto;
	float: left;
}
.set_dynBlockWrapper
{
	width: inherit;
	height: inherit;
	position: absolute;
	float: left;
}

.set_block
{
	width: inherit;
	min-height: $global::set_lineSize$;
	overflow: auto;
	float: left;
	border-bottom: 3;
	margin-top: 1;
}

.set_line
{
	width: inherit;
	height: $global::set_lineSize$;
	border-bottom: 3;
	margin-left: 4;
	margin-right: 4;
	float: left;
}

.set_expandLine
{
	width: inherit;
	min-height: $global::set_lineSize$;
	overflow-Y: auto;
	float: left;
	border-bottom: 3;
	margin-right: 4;
}

.set_vertLine
{
	width: $global::set_lineSize$;
	height: inherit;
	border-right: 3;
	margin-top: 4;
	margin-bottom: 4;
	float: left;
}

.set_divider
{
	width: inherit;
	height: 1;
	border-bottom: 3;
	margin-right: 4;
	margin-left: 4;
	background-color: $global::midGrey$;
}
.set_headerLine
{
	width: 130;
	height: 1;
	float: left;
	margin-top: 8;
	margin-right: 4;
	margin-left: 4;
	background-color: $global::midGrey$;
}


// Tabs
.set_tabBar {
	width: inherit;
	height: 20;
	margin-left: 2;
	float: left;
	background-color: $global::darkGrey$;
}

.set_tab {
	min-width: 42;
	height: 18;
	overflow-X: auto;
	float: left;
	border-right: 3;
	margin-top: 2;
	background-color: $global::lowMidGrey$;
}

.set_tabText {
	min-width: 12;
	float: left;
	margin-top: 2;
	margin-left: 8;
	margin-right: 8;
	border-right: 4;
	border-left: 4;
	fill-color: $midGrey$;
}

// Buttons
.set_closeButton_s {
