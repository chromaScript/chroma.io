// Text

.set_text {
	margin-top: 1;
	min-width: 8;
	border-right: 4;
	border-left: 4;
}
.set_inBox {
	width: inherit;
	height: inherit;
	border-right: 2;
	margin-right: 4;
	background-color: $global::darkGreyLight$;
}
.set_inText {
	width: inherit;
	margin-top: 1;
	margin-left: 3;
}

// Sliders

.set_slRail {
	width: fill;
	height: 2;
	margin-top: 8;
	margin-left: 2;
	border-right: 2;
	background-color: $global::midGrey$;
}
.set_slVertRail {
	width: 2;
	height: fill;
	margin-left: 8;
	margin-top: 2;
	border-bottom: 2;
	background-color: $global::midGrey$;
}

.set_slBox_minMax {
	width: fill;
	height: inherit;
	border-right: 4;
	float: left;
}
.set_slBox_single {
	width: fill;
	height: inherit;
	border-right: 4;
	float: left;
}
.set_slBox_vertSingle {
	width: inherit;
	height: fill;
	border-bottom: 4;
	float: left;
}

.set_sl_single {
	width: $global::sl_singleWidth$;
	height: 12;
	position: absolute;
	margin-top: 3;
	background-color: $global::midGrey$;
	box-shadow-color: black;
	box-shadow-size: 1, 1;
}
.set_sl_vertSingle {
	width: 12;
	height: 6;
	position: absolute;
	margin-left: 3;
	background-color: $global::midGrey$;
	box-shadow-color: black;
	box-shadow-size: 1, 1;
}

.set_sl_min {
	width: $global::sl_doubleWidth$;
	height: 8;
	position: absolute;
	background-texture: "textures/T_slider_carrot6x8_min_D.png";
	background-color-alpha: 1;
	fill-color: $global::midGrey$;
	box-shadow-color: black;
	box-shadow-size: 1, 1;
	float: left;
	margin-top: 11;
}
.set_sl_max {
	width: $global::sl_doubleWidth$;
	height: 8;
	position: absolute;
	background-texture: "textures/T_slider_carrot6x8_max_D.png";
	background-color-alpha: 1;
	fill-color: $global::midGrey$;
	box-shadow-color: black;
	box-shadow-size: 1, 1;
	float: left;
	margin-top: 1;
}

// Drop Menus

// Drop Menu

.set_dropWrapper {
	min-width: 24;
	height: inherit;
	
	overflow-X: auto;
	background-color: $global::darkGreyLight$;
}
.set_dropText {
	margin-left: 3;
	margin-top: 1;
	fill-color: $global::midGrey$;
}

.set_sub {
	margin-left: 3;
	margin-top: -1;
	z-index: 2;
}

.set_dropTop {
	width: inherit;
	height: inherit;
}

.set_dropItem {
	width: inherit;
	height: 14;
	margin-top: 2;
	margin-bottom: 4;
	border-bottom: 2;
	z-index: 2;
}

.set_dropList {
	position: absolute;
	width: inherit;
	min-height: 6;
	top: 18;
	overflow-Y: auto;
	background-color: $global::darkGrey$;
	z-index: 2;
	visibility: hide;
}

.set_dropButton {
	width: 12;
	height: 12;
	border-left: 3;
	margin-top: 3;
	margin-right: 3;
	float: right;
	background-texture: "textures/T_arrowDown36_D.png";
	background-color-alpha: 1;
}