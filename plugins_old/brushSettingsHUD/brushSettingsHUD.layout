<ROOT id="brushSettingsHUD" defaultPosition="100,100">
<V_BOX id="bshud_wrapper" ondragstart="movePopup(0)" ondrag="movePopup(1)">
	<H_BOX class="settingsSlideWrapper">
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="S:"></TEXT></H_BOX>
		<H_BOX class="settingsSlideBox" 
			ondragstart="dragSettings(0, 0)" 
			ondrag="dragSettings(0, 0)" 
			ondragend="dragSettings(1, 0)">
			<H_BOX class="sizeSlideLine"></H_BOX>
			<H_BOX id="sizeSlider" class="slideWidget"></H_BOX>
		</H_BOX>
	</H_BOX>
	<H_BOX class="settingsSlideWrapper">
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="VS:"></TEXT></H_BOX>
		<H_BOX class="settingsSlideBox" 
			ondragstart="dragSettings(0, 1)" 
			ondrag="dragSettings(0, 1)" 
			ondragend="dragSettings(1, 1)">
			<H_BOX class="sizeSlideLine"></H_BOX>
			<H_BOX id="vertexSpacingSlider" class="slideWidget"></H_BOX>
		</H_BOX>
	</H_BOX>
	<H_BOX class="settingsSlideWrapper" style="border-bottom:2;">
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="BS:"></TEXT></H_BOX>
		<H_BOX class="settingsSlideBox" 
			ondragstart="dragSettings(0, 2)" 
			ondrag="dragSettings(0, 2)" 
			ondragend="dragSettings(1, 2)">
			<H_BOX class="sizeSlideLine"></H_BOX>
			<H_BOX id="brushSpacingSlider" class="slideWidget"></H_BOX>
		</H_BOX>
	</H_BOX>
	<H_BOX class="transferWrapper">
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="DYNAMIC:"></TEXT></H_BOX>
		<H_BOX class="bshudCheckWrapper" onclick="toggleCheck(0)"><H_BOX id="opacityDynamic" class="bshudCheckBox"></H_BOX></H_BOX>
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="MIN:"></TEXT></H_BOX>
		<H_BOX class="percentDragWrapper"
			ondragstart="dragPercents(0, 0)" 
			ondrag="dragPercents(1, 0)" 
			ondragend="dragPercents(2, 0)">
			<TEXT class="bshudText" value="O:"></TEXT>
			<TEXT id="minOpacitySlider" class="bshudText" value="100%"></TEXT>
			<IMAGE class="bshudArrows"></IMAGE>
		</H_BOX>
		<H_BOX class="percentDragWrapper"
			ondragstart="dragPercents(0, 2)" 
			ondrag="dragPercents(1, 2)" 
			ondragend="dragPercents(2, 2)">
			<TEXT class="bshudText" value="F:"></TEXT>
			<TEXT id="minFlowSlider" class="bshudText" value="100%"></TEXT>
			<IMAGE class="bshudArrows"></IMAGE>
		</H_BOX>
	</H_BOX>
	<H_BOX class="transferWrapper">
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="DYNAMIC:"></TEXT></H_BOX>
		<H_BOX class="bshudCheckWrapper" onclick="toggleCheck(1)"><H_BOX id="flowDynamic" class="bshudCheckBox"></H_BOX></H_BOX>
		<H_BOX class="sliderTextWrapper"><TEXT class="bshudText" value="MAX:"></TEXT></H_BOX>
		<H_BOX class="percentDragWrapper"
			ondragstart="dragPercents(0, 1)" 
			ondrag="dragPercents(1, 1)" 
			ondragend="dragPercents(2, 1)">
			<TEXT class="bshudText" value="O:"></TEXT>
			<TEXT id="maxOpacitySlider" class="bshudText" value="100%"></TEXT>
			<IMAGE class="bshudArrows"></IMAGE>
		</H_BOX>
		<H_BOX class="percentDragWrapper"
			ondragstart="dragPercents(0, 3)" 
			ondrag="dragPercents(1, 3)" 
			ondragend="dragPercents(2, 3)">
			<TEXT class="bshudText" value="F:"></TEXT>
			<TEXT id="maxFlowSlider" class="bshudText" value="100%"></TEXT>
			<IMAGE class="bshudArrows"></IMAGE>
		</H_BOX>
	</H_BOX>
</V_BOX>
</ROOT>